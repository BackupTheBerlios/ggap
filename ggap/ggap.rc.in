	VALUE "CompanyName", "Yevgen Muntyan"
	VALUE "FileDescription", "GGAP"
	VALUE "FileVersion", "@GGAP_VERSION_UNQUOTED@.0"
	VALUE "InternalName", "ggap-@GGAP_VERSION_UNQUOTED@"
	VALUE "LegalCopyright", "(C) 2004-2008 Yevgen Muntyan <muntyan@tamu.edu>"
	VALUE "OriginalFilename", "ggap.exe"
	VALUE "ProductName", "GGAP"
	VALUE "ProductVersion", @GGAP_VERSION@
      END
    END
    BLOCK "VarFileInfo"
    BEGIN
      VALUE "Translation", 0x409, 1200
    END
  END
